<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
   <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#734d9115" />
            <corners
                android:radius="5dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <corners
                android:radius="5dp"/>
            <stroke
                android:color="#4d9115"
                android:width="1dp"/>
        </shape>
    </item>
</selector>
